{
  "name": "@exchange/sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "generate:openapi": "openapi-typescript ../../packages/shared/openapi.json -o src/types/generated.ts",
    "generate:websocket": "bun ./scripts/generate-types.ts",
    "generate": "bun run generate:openapi && bun run generate:websocket",
    "build": "bun run generate && tsc",
    "dev": "bun run generate && tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "json-schema-to-typescript": "^15.0.4",
    "openapi-typescript": "^7.4.4",
    "typescript": "^5.6.3"
  },
  "dependencies": {}
}
