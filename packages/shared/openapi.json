                "schema": {
                  "$ref": "#/components/schemas/DripErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DripErrorResponse"
                }
              }
            }
          }
        }
      }
    },
